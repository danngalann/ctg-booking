<ul class="collapsible popout" style="position:relative;">
    {% for booking in bookings %}
    <li>
        <div class="collapsible-header">
            {{ booking.name }}
            ({{ booking.startTime|date('H:i') }} - {{ booking.endTime|date('H:i') }})
            <span class="client-number">
                <sup>{{ booking.clients|length }}</sup>
                <i class="material-icons">person</i>
            </span>

        </div>
        <div data-booking-id="{{ booking.id }}" class="collapsible-body">
            {%
                include 'backend_home/includes/_booking_client_table.html.twig' with
                {
                    "clients": booking.clients
                }
            %}
        </div>
    </li>
    {% endfor %}
</ul>