{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col s12 m6 offset-m3">
            {%
                include 'backend_home/includes/_booking_collapsible.html.twig' with
                {
                    "bookings": bookings
                }
            %}
        </div>
    </div>
{% endblock %}
{% block bodyscripts %}
    {{ parent() }}
    <script>
        {# Init collapsible #}
        $(document).ready(function(){
            $('.collapsible').collapsible();
        });
    </script>
{% endblock %}